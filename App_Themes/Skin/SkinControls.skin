</asp:TextBox>

<asp:TextBox runat="server" SkinId="TextBoxDefault" CssClass="textboxSolo" >
</asp:TextBox>

<%--      SKIN PARA EL TEXTBOX, ESTILO POR DEFECTO       --%>
<asp:TextBox runat="server" SkinId="TextBoxDefaultDisable"
   CssClass="disable" 
   Height="14px"
   BackColor="#EBEBEB" >
</asp:TextBox>

<%--  skin para los textbox --%>
 <asp:TextBox runat="server"  CssClass="TextBox" SkinId="xoloTextbox"
 BorderColor ="#a4c8fa"
 BorderStyle="solid" 
 BorderWidth="1px" 
 Font-Names="Verdana"
 Font-Size="10px" 
 ForeColor="#333333" 
 height="12px"/>
 
  <%--  skin para los textbox de sms --%>
  <asp:TextBox runat="server" SkinId="xoloTextboxSMS"  CssClass="TextBox"
 BorderColor ="#a4c8fa"
 BorderStyle="solid" 
 BorderWidth="1px" 
 Font-Names="Verdana"
 Font-Size="10px" 
 ForeColor="#333333" 
 height="12px"/>
 
 
  <%--  skin para los textbox que es No Editable --%>
<asp:TextBox runat="server" SkinId="xolotextboxnoeditable"  CssClass="TextBox"
 BorderColor ="#a4c8fa"
 BorderStyle="solid" 
 BorderWidth="1px" 
 Font-Names="Verdana"
 Font-Size="10px" 
 ForeColor="#333333" 
 BackColor="#EBEBEB"
 height="12px"/>
 
  <%--  skin para los textbox con cantidades numericas --%>
 <asp:TextBox runat="server" SkinId="xolotextboxnumeros"  CssClass="TextBoxNum"
 BorderColor ="#a4c8fa"
 BorderStyle="solid" 
 BorderWidth="1px" 
 Font-Names="Verdana"
 Font-Size="14px" 
 ForeColor="#333333" />
 
  <%--  skin para los textbox que es No Editable con cantidades numericas --%>
<asp:TextBox runat="server" SkinId="xolotextboxnoeditablenumeros"  CssClass="TextBoxNum"
 BorderColor ="#a4c8fa"
 BorderStyle="solid" 
 BorderWidth="1px" 
 Font-Names="Verdana"
 Font-Size="10px" 
 ForeColor="#333333" 
 BackColor="#EBEBEB"/>
  
<%--  skin para los textbox cuando son textarea --%>
<asp:TextBox runat="server" SkinId="xolotextarea" 
BorderColor ="#a4c8fa"
BorderStyle="solid"
BorderWidth="1px" 
ForeColor="#333333"/>

