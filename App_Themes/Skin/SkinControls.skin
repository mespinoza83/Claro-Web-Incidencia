<%--SKIN PARA EL GRIDVIEW, COLOR AZUL GRIDVIEWDEFAULT--%> 
<asp:GridView runat="server" AutoGenerateColumns="False" 
  Width="100%" AllowPaging="True" ShowFooter="False" PageSize="10" CellSpacing="0"
